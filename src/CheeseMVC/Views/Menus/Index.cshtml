@model List<CheeseMVC.Models.CheeseMenu>
    @{
        ViewData["Title"] = "Index";
    }

    <h2>Menu</h2>

   @if (Model.Count==0)
   {
    <p>
        No Menus Yet
    </p>
    }

    <ul>
        @foreach (var menu in Model)
        {
           <li> @menu.MenuID</li>
        }
    </ul>
<p>
    <a asp-controller="Menu" asp-action="Add" >Add Menu</a> 
</p>

